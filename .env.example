# App Configuration
NEXT_PUBLIC_APP_NAME=RAWGLE
NEXT_PUBLIC_APP_URL=https://rawgle.com

# API Configuration
NEXT_PUBLIC_API_URL=https://api.rawgle.com
API_KEY=your-api-key-here

# Cloudflare Configuration
CF_ACCOUNT_ID=your-cloudflare-account-id
CF_API_TOKEN=your-cloudflare-api-token
CF_KV_NAMESPACE_ID=your-kv-namespace-id
CF_R2_BUCKET=rawgle-assets
CF_DURABLE_OBJECT_NAMESPACE=rawgle-do

# Solana Configuration
NEXT_PUBLIC_SOLANA_NETWORK=mainnet-beta
NEXT_PUBLIC_SOLANA_RPC_URL=https://api.mainnet-beta.solana.com
PAWS_TOKEN_MINT=your-paws-token-mint-address
PAWS_PROGRAM_ID=your-paws-program-id

# Authentication
NEXTAUTH_URL=https://rawgle.com
NEXTAUTH_SECRET=your-nextauth-secret-here

# Database (Supabase Fallback)
SUPABASE_URL=your-supabase-url
SUPABASE_ANON_KEY=your-supabase-anon-key
SUPABASE_SERVICE_KEY=your-supabase-service-key

# Third-party Services
NEXT_PUBLIC_MAPBOX_TOKEN=your-mapbox-token
NEXT_PUBLIC_GA_ID=G-XXXXXXXXXX
STRIPE_SECRET_KEY=sk_live_xxxxx
STRIPE_WEBHOOK_SECRET=whsec_xxxxx
OPENAI_API_KEY=sk-xxxxx

# Gelato Print-on-Demand
GELATO_API_KEY=your-gelato-api-key
GELATO_API_URL=https://api.gelato.com/v1
GELATO_WEBHOOK_SECRET=your-gelato-webhook-secret

# Email Service
RESEND_API_KEY=re_xxxxx
EMAIL_FROM=noreply@rawgle.com

# AWS (Fallback Services)
AWS_ACCESS_KEY_ID=your-aws-access-key
AWS_SECRET_ACCESS_KEY=your-aws-secret-key
AWS_REGION=us-east-1
AWS_S3_BUCKET=rawgle-backups

# Feature Flags
NEXT_PUBLIC_ENABLE_PAWS=true
NEXT_PUBLIC_ENABLE_CHAT=true
NEXT_PUBLIC_ENABLE_ANALYTICS=true
NEXT_PUBLIC_MAINTENANCE_MODE=false

# Development
NODE_ENV=production